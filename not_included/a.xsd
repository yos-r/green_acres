                                                                <xs:simpleType>
                                                                    <xs:restriction
                                                                        base="xs:integer" />
                                                                </xs:simpleType>
                                                                <xs:simpleType>
                                                                    <xs:restriction base="xs:string">
                                                                        <xs:enumeration value="" />
                                                                    </xs:restriction>
                                                                </xs:simpleType>
                                                            </xs:union>
                                                        </xs:simpleType>
                                                    </xs:element>
