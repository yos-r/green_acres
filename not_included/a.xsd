<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" attributeFormDefault="unqualified"
    elementFormDefault="qualified">
    <xs:element name="Envelope">
        <xs:complexType>
            <xs:all>
                <xs:element name="Body">
                    <xs:complexType>
                        <xs:all>
                            <xs:element name="add_adverts">
                                <xs:complexType>
                                    <xs:sequence>
                                        <xs:element name="advert" maxOccurs="unbounded">
                                            <xs:complexType>
                                                <xs:all>
                                                    <xs:element ref="acc" />
                                                    <xs:element name="transfert_id" minOccurs="0"
                                                        maxOccurs="1">
                                                        <xs:annotation>
                                                            <xs:documentation>identifiant éditeur de
                                                                l'agence</xs:documentation>
                                                        </xs:annotation>
                                                        <xs:simpleType>
                                                            <xs:restriction base="xs:string" />
                                                        </xs:simpleType>
                                                    </xs:element>
                                                    <xs:element name="reference">
                                                        <xs:annotation>
                                                            <xs:documentation>référence green-acres
                                                                du bien</xs:documentation>
                                                        </xs:annotation>
                                                        <xs:simpleType>
                                                            <xs:restriction base="xs:string" />
                                                        </xs:simpleType>
                                                    </xs:element>
                                                    <xs:element name="advert_type">
                                                        <xs:annotation>
                                                            <xs:documentation>destination du bien :
                                                                vente, location ou location vacances</xs:documentation>
                                                        </xs:annotation>
                                                        <xs:simpleType>
                                                            <xs:restriction base="xs:string">
                                                                <xs:enumeration value="properties" />
                                                                <xs:enumeration value="rentals" />
                                                            </xs:restriction>
                                                        </xs:simpleType>
                                                    </xs:element>
                                                    <xs:element name="status" minOccurs="0">
                                                        <xs:annotation>
                                                            <xs:documentation>statut du bien :
                                                                disponible, sous compromis, vendu,
                                                                loué, expiré</xs:documentation>
                                                        </xs:annotation>
                                                        <xs:simpleType>
                                                            <xs:restriction base="xs:string">
                                                                <xs:enumeration value="available" />
                                                                <xs:enumeration value="under_offer" />
                                                                <xs:enumeration value="sold" />
                                                                <xs:enumeration value="rented" />
                                                                <xs:enumeration value="expired" />
                                                            </xs:restriction>
                                                        </xs:simpleType>
                                                    </xs:element>
                                                    <xs:element name="price">
                                                        <xs:annotation>
                                                            <xs:documentation>prix du bien, charges
                                                                incluses</xs:documentation>
                                                        </xs:annotation>
                                                        <xs:simpleType>
                                                            <xs:restriction base="xs:int">
                                                                <xs:minInclusive value="1" />
                                                            </xs:restriction>
                                                        </xs:simpleType>
                                                    </xs:element>
                                                    <xs:element name="postal_code">
                                                        <xs:annotation>
                                                            <xs:documentation>Code postal de
                                                                l'adresse du bien</xs:documentation>
                                                        </xs:annotation>
                                                        <xs:simpleType>
                                                            <xs:restriction base="xs:string">
                                                                <xs:maxLength value="10" />
                                                            </xs:restriction>
                                                        </xs:simpleType>
                                                    </xs:element>
                                                    <xs:element name="currency">
                                                        <xs:simpleType>
                                                            <xs:restriction base="xs:string">
                                                                <xs:length value="3" />
                                                            </xs:restriction>
                                                        </xs:simpleType>
                                                    </xs:element>
                                                    <xs:element name="department" minOccurs="0" />
                                                    <xs:element name="region" minOccurs="0" />
                                                    <xs:element name="city">
                                                        <xs:simpleType>
                                                            <xs:restriction base="xs:string">
                                                                <xs:maxLength value="75" />
                                                                <xs:minLength value="1" />
                                                            </xs:restriction>
                                                        </xs:simpleType>
                                                    </xs:element>
                                                    <xs:element name="location_id" minOccurs="0"
                                                        maxOccurs="1">
                                                        <xs:simpleType>
                                                            <xs:restriction base="xs:int" />
                                                        </xs:simpleType>
                                                    </xs:element>
                                                    <xs:element name="software" minOccurs="0"
                                                        maxOccurs="1">
                                                        <xs:simpleType>
                                                            <xs:restriction base="xs:string" />
                                                        </xs:simpleType>
                                                    </xs:element>
                                                    <xs:element name="country_code">
                                                        <xs:annotation>
                                                            <xs:documentation>Pays où est situé le
                                                                bien</xs:documentation>
                                                        </xs:annotation>
                                                        <xs:simpleType>
                                                            <xs:restriction base="xs:string">
                                                                <xs:length value="2" />
                                                            </xs:restriction>
                                                        </xs:simpleType>
                                                    </xs:element>
                                                    <xs:element name="property_type">
                                                        <xs:annotation>
                                                            <xs:documentation>Type de bien</xs:documentation>
                                                        </xs:annotation>
                                                        <xs:simpleType>
                                                            <xs:restriction base="xs:string">
                                                                <xs:enumeration value="old" />
                                                                <xs:enumeration value="recent" />
                                                                <xs:enumeration value="house" />
                                                                <xs:enumeration value="castle" />
                                                                <xs:enumeration value="land" />
                                                                <xs:enumeration value="appartement" />
                                                                <xs:enumeration value="business" />
                                                                <xs:enumeration value="parking" />
                                                                <xs:enumeration value="new" />
                                                                <xs:enumeration value="gite" />
                                                                <xs:enumeration
                                                                    value="bed_breakfast" />
                                                                <xs:enumeration value="mobile_home" />
                                                                <xs:enumeration value="fazenda" />
                                                                <xs:enumeration value="building" />
                                                                <xs:enumeration value="other" />
                                                            </xs:restriction>
                                                        </xs:simpleType>
                                                    </xs:element>
                                                    <xs:element name="dpe_date" minOccurs="0"> </xs:element>
                                                    <xs:element name="is_favorite" minOccurs="0"> </xs:element>
                                                    <xs:element name="ges_date" minOccurs="0"> </xs:element>
                                                    <xs:element name="dpe_value" minOccurs="0"
                                                        maxOccurs="1">
                                                        <xs:annotation>
                                                            <xs:documentation>Bilan énergétique du
                                                                bien</xs:documentation>
                                                        </xs:annotation>
                                                        <xs:simpleType>
                                                            <xs:union>
                                                                <xs:simpleType>
                                                                    <xs:restriction
                                                                        base="xs:decimal" />
                                                                </xs:simpleType>
                                                                <xs:simpleType>
                                                                    <xs:restriction base="xs:string">
                                                                        <xs:enumeration value="" />
                                                                    </xs:restriction>
                                                                </xs:simpleType>
                                                            </xs:union>
                                                        </xs:simpleType>
                                                    </xs:element>
                                                    <xs:element name="ges_value" minOccurs="0"
                                                        maxOccurs="1">
                                                        <xs:annotation>
                                                            <xs:documentation>Bilan gaz à effet de
                                                                serre</xs:documentation>
                                                        </xs:annotation>
                                                        <xs:simpleType>
                                                            <xs:union>
                                                                <xs:simpleType>
                                                                    <xs:restriction
                                                                        base="xs:decimal" />
                                                                </xs:simpleType>
                                                                <xs:simpleType>
                                                                    <xs:restriction base="xs:string">
                                                                        <xs:enumeration value="" />
                                                                    </xs:restriction>
                                                                </xs:simpleType>
                                                            </xs:union>
                                                        </xs:simpleType>
                                                    </xs:element>
                                                    <xs:element name="dpe_type" minOccurs="0">
                                                        <xs:annotation>
                                                            <xs:documentation>Bilan énergétique du
                                                                bien (lettre)</xs:documentation>
                                                        </xs:annotation>
                                                        <xs:simpleType>
                                                            <xs:restriction base="xs:string">
                                                                <xs:enumeration value="" />
                                                                <xs:enumeration value="A" />
                                                                <xs:enumeration value="B" />
                                                                <xs:enumeration value="C" />
                                                                <xs:enumeration value="D" />
                                                                <xs:enumeration value="E" />
                                                                <xs:enumeration value="F" />
                                                                <xs:enumeration value="G" />
                                                            </xs:restriction>
                                                        </xs:simpleType>
                                                    </xs:element>
                                                    <xs:element name="ges_type" minOccurs="0">
                                                        <xs:annotation>
                                                            <xs:documentation>Bilan gaz à effet de
                                                                serre (lettre)</xs:documentation>
                                                        </xs:annotation>
                                                        <xs:simpleType>
                                                            <xs:restriction base="xs:string">
                                                                <xs:enumeration value="" />
                                                                <xs:enumeration value="A" />
                                                                <xs:enumeration value="B" />
                                                                <xs:enumeration value="C" />
                                                                <xs:enumeration value="D" />
                                                                <xs:enumeration value="E" />
                                                                <xs:enumeration value="F" />
                                                                <xs:enumeration value="G" />
                                                            </xs:restriction>
                                                        </xs:simpleType>
                                                    </xs:element>
                                                    <xs:element name="structure" minOccurs="0">
                                                        <xs:annotation>
                                                            <xs:documentation>Conditions de gros
                                                                oeuvre : 0- structure/extérieur en
                                                                bon état, 1- A rafraîchir, 2-
                                                                structure/extérieur à restaurer</xs:documentation>
                                                        </xs:annotation>
                                                        <xs:simpleType>
                                                            <xs:restriction base="xs:string">
                                                                <xs:enumeration value="?" />
                                                                <xs:enumeration value="0" />
                                                                <xs:enumeration value="1" />
                                                                <xs:enumeration value="2" />
                                                                <xs:enumeration value="" />
                                                            </xs:restriction>
                                                        </xs:simpleType>
                                                    </xs:element>
                                                    <xs:element name="mandate_number" minOccurs="0">
